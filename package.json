{
  "private": true,
  "name": "next-blog",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "codegen": "yarn graphql-codegen && yarn json-schema-codegen",
    "dev": "next-remote-watch ./content",
    "graphql-codegen": "graphql-codegen -r dotenv/config",
    "install": "yarn codegen",
    "json-schema-codegen": "curl -sS https://qiita.com/api/v2/schema | yarn json2ts -o src/generated/qiita.ts --bannerComment '/* eslint-disable */'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "start": "next start",
    "svg": "npx @svgr/cli --out-dir src/components/icons --typescript -- public/icons && yarn eslint 'src/components/icons/*.{ts,tsx}' --fix",
    "type-check": "tsc"
  },
  "dependencies": {
    "feed": "4.2.2",
    "glob": "8.0.1",
    "graphql-request": "3.6.1",
    "gray-matter": "4.0.3",
    "highlight.js": "11.3.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-datocms": "1.6.9",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "rehype-highlight": "4.1.0",
    "rehype-react": "6.2.1",
    "remark-parse": "9.0.0",
    "remark-rehype": "8.1.0",
    "tsyringe": "4.6.0",
    "unified": "9.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.18.6",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-graphql-request": "4.3.3",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@types/glob": "7.2.0",
    "@types/node": "14.18.16",
    "@types/react": "17.0.44",
    "autoprefixer": "10.4.4",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "7.32.0",
    "eslint-config-thai-soup": "1.1.0",
    "graphql": "15.8.0",
    "husky": "7.0.4",
    "json-schema-to-typescript": "10.1.5",
    "lint-staged": "11.2.6",
    "next-remote-watch": "1.0.0",
    "postcss": "8.4.12",
    "prettier": "2.4.1",
    "prettier-package-json": "2.6.4",
    "tailwindcss": "3.0.24",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{ts.tsx}": "eslint --fix",
    "*.graphql": "prettier --write",
    "package.json": "prettier-package-json --write"
  }
}
